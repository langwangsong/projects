<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.itcast.crm.domain.CourseType" table="crm_coursetype">
		<id name="tid" column="tid">
			<generator class="native"></generator>
		</id>
		<property name="tname" column="tname" length="20"/>
		<property name="tdesc" column="tdesc" length="100"/>
		<property name="tnum" column="tnum"/>
		<property name="tprice" column="tprice"/>
	</class>
</hibernate-mapping>                                